<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventplanner">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AtoZEventPlanning"></activity>
        <activity android:name=".CommanQuestion" />
        <activity android:name=".DetailsAboutEvents" />
        <activity android:name=".ListofEvents" />
        <activity android:name=".DataAboutArtist" />
        <activity android:name=".ArtistList" />
        <activity android:name=".ProfileUpdate" />
        <activity android:name=".ForgetPassword" />
        <activity android:name=".UserMain" />
        <activity android:name=".Registration">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".SplashScreen" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>